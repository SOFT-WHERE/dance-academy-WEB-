extends base.pug

block scripts
    script(src='/static/index.js')
block style 
    style
      include ../static/style.css
      include ../static/stylecontact.css

block content 
    div.container
        h1 Contact Here
        form(action="/contact", method="post" class="myform")
            input(type="text", class="myinput", name="Name", placeholder="Name")
            input(type="email", class="myinput", name="Email", placeholder="Email")
            input(type="phone", class="myinput", name="Phone", placeholder="Phone")
            input(type="text", class="myinput", name="Address", placeholder="Address")
        button(class="btn")
            |SUBMIT